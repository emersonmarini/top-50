{
	"name": "emerson-marini",
	"version": "0.1.0",
	"description": "Testing app",
	"author": "Emerson Marini <emerson.marini@gmail.com>",
	"license": "ISC",
	"private": "true",
	"main": "./src/index.tsx",
	"engines": {
		"node": "18.16.1",
		"npm": "9.5.1"
	},
	"scripts": {
		"build:cjs": "tsc --project ./tsconfig.cjs.json",
		"build:esm": "tsc --project ./tsconfig.esm.json",
		"build:pack": "npm run clean:dist && npm run build:cjs && npm run build:esm",
		"build": "cross-env NODE_ENV=production webpack",
		"clean:dist": "rimraf ./dist && mkdir dist",
		"eslint:fix": "eslint --ext .js,.jsx,.ts,.tsx webpack.config.ts ./src --color --cache --fix",
		"eslint:watch": "npm run lint -- --watch",
		"eslint": "eslint --ext .js,.jsx,.ts,.tsx webpack.config.ts ./src --color --cache",
		"lint:fix": "npm run eslint:fix && npm run lint:styles:fix",
		"lint:styles:fix": "stylelint \"src/**/*.less\" --fix",
		"lint:styles": "stylelint \"src/**/*.less\"",
		"lint": "npm run eslint && npm run lint:styles",
		"prebuild": "npm run clean:dist",
		"prettier:fix": "npx prettier --write **/*",
		"prettier": "npx prettier --check **/*",
		"start": "webpack serve --env development",
		"test": "echo \"Error: no test specified\" && exit 1",
		"types": "tsc --project ./tsconfig.json",
		"validate:fix": "npm run types && npm run lint:fix && npm run prettier:fix",
		"validate": "npm run types && npm run lint && npm run prettier"
	},
	"dependencies": {
		"core-js": "3.32.0",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-intl": "6.4.4"
	},
	"devDependencies": {
		"@babel/core": "7.22.9",
		"@babel/plugin-transform-react-constant-elements": "^7.22.5",
		"@babel/plugin-transform-react-inline-elements": "^7.22.5",
		"@babel/plugin-transform-runtime": "7.22.9",
		"@babel/preset-env": "7.22.9",
		"@babel/preset-react": "7.22.5",
		"@babel/preset-typescript": "7.22.5",
		"@pmmmwh/react-refresh-webpack-plugin": "0.5.10",
		"@types/node": "20.4.6",
		"@types/react": "18.2.18",
		"@types/react-dom": "18.2.7",
		"@types/webpack": "5.28.1",
		"@typescript-eslint/eslint-plugin": "6.2.1",
		"@typescript-eslint/parser": "6.2.1",
		"babel-loader": "9.1.3",
		"babel-plugin-formatjs": "10.5.3",
		"babel-plugin-transform-react-remove-prop-types": "^0.4.24",
		"check-node-version": "4.2.1",
		"clean-webpack-plugin": "4.0.0",
		"copy-webpack-plugin": "11.0.0",
		"cross-env": "7.0.3",
		"css-loader": "6.8.1",
		"css-minimizer-webpack-plugin": "5.0.1",
		"eslint": "8.46.0",
		"eslint-config-prettier": "8.10.0",
		"eslint-import-resolver-webpack": "0.13.2",
		"eslint-plugin-import": "2.28.0",
		"eslint-plugin-jsx-a11y": "6.7.1",
		"eslint-plugin-prettier": "5.0.0",
		"eslint-plugin-react": "7.33.1",
		"eslint-plugin-react-hooks": "4.6.0",
		"html-webpack-plugin": "5.5.3",
		"less": "4.1.3",
		"less-loader": "11.1.3",
		"mini-css-extract-plugin": "2.7.6",
		"postcss-less": "6.0.0",
		"prettier": "3.0.1",
		"react-refresh": "^0.14.0",
		"rimraf": "5.0.1",
		"style-loader": "3.3.3",
		"stylelint": "15.10.2",
		"stylelint-prettier": "4.0.2",
		"ts-loader": "9.4.4",
		"ts-node": "10.9.1",
		"typescript": "5.1.6",
		"webpack": "5.88.2",
		"webpack-cli": "5.1.4",
		"webpack-dev-server": "4.15.1",
		"webpackbar": "5.0.2"
	}
}
